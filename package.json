{
  "name": "react-isomorphic-render",
  "version": "4.0.1",
  "description": "Isomorphic rendering with React, Redux, React-router and Redux-router. Includes support for Webpack",
  "main": "index.umd.js",
  "//jsnext:main": "index.es6.js",
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "history": "^2.0.1",
    "hoist-non-react-statics": "^1.0.5",
    "koa": "1.1.2",
    "koa-locale": "^1.0.0",
    "print-error": "^0.1.3",
    "react-helmet": "^3.1.0",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-router": "^2.0.0",
    "scroll-behavior": "^0.7.0",
    "superagent": "^1.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "chai": "^3.5.0",
    "npm-run-all": "^1.4.0",
    "rimraf": "^2.5.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0",
    "react-router": "^2.0.0",
    "redux": "^3.0.0"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --colors --bail --reporter spec test/ --recursive",
    "test-coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register --colors --reporter dot test/ --recursive",
    "test-travis": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --compilers js:babel-core/register --colors --reporter spec test/ --recursive",
    "gh-pages": "webpack --action=gh-pages --colors --config ./config",
    "deploy-gh-pages": "TARGET=gh-pages node ./config/deploy-gh-pages.js",
    "clean-for-build": "rimraf ./babel-transpiled-modules/**/*",
    "build-modules": "babel ./source --out-dir ./babel-transpiled-modules --source-maps",
    "build": "npm-run-all clean-for-build build-modules",
    "commit-new-build": "git commit --all --message=\"Update build\"",
    "prepublish": "npm-run-all test build",
    "watch": "babel scripts/watch.js | node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halt-hammerzeit/react-isomorphic-render.git"
  },
  "keywords": [
    "react",
    "isomorphic",
    "redux"
  ],
  "author": "Halt Hammerzeit <halt.hammerzeit.at@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halt-hammerzeit/react-isomorphic-render/issues"
  },
  "homepage": "https://github.com/halt-hammerzeit/react-isomorphic-render#readme"
}
